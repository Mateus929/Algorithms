#include<bits/stdc++.h>
#define INF 1e9

using namespace std;

int lis(vector<int> &a){
    int n=a.size();
    vector<int> d(n+1,INF);
    d[0]=-INF;
    for(int i=0;i<n;i++){
        int j=upper_bound(d.begin(),d.end(),a[i])-d.begin();
        if(d[j-1]<a[i] && a[i]<d[j])
            d[j]=a[i];
    }
    int ans=0;
    for(int i=0;i<=n;i++)
        if(d[i]<INF)
             ans=i;
    return ans;
}
int main(){
    int n;
    cin >> n;
    vector<int> a(n);
    for(int i=0;i<n;i++)
        cin >> a[i];
    cout << lis(a);
}
